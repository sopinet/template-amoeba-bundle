<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Amoeba free one page responsive bootstrap site template{% endblock %}</title>
    
    {#<link rel="shortcut icon" href="http://work.sopinet.es/icofrade-web/web/uploads/favicon.ico" />#}
    {% block css %}
	   	{% stylesheets
	   		'@SopinetTemplateAmoebaBundle/Resources/public/css/isotope.css'
	   		'@SopinetTemplateAmoebaBundle/Resources/public/js/fancybox/jquery.fancybox.css'
	    	'@SopinetTemplateAmoebaBundle/Resources/public/css/bootstrap.css'
	    	'@SopinetTemplateAmoebaBundle/Resources/public/css/bootstrap-theme.css'
	    	'@SopinetTemplateAmoebaBundle/Resources/public/css/style.css'
	    	'@SopinetTemplateAmoebaBundle/Resources/public/skin/default.css'
	    	
	    	'bundles/sopinettemplateamoeba/css/font-awesome.css'
	    	output='css/*.css' filter='cssrewrite' %}
	    	<link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
		{% endstylesheets %}
    {% endblock %}

</head>
	{% block custombody %}<body>{% endblock %}
	{% block body %}
	    <h1>Hello, world!</h1>
	{% endblock %}

{% block js %}
	{% javascripts 
		'@SopinetTemplateAmoebaBundle/Resources/public/js/modernizr-2.6.2-respond-1.1.0.min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.easing.1.3.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/bootstrap.min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.isotope.min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.nicescroll.min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/fancybox/jquery.fancybox.pack.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/skrollr.min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.scrollTo-1.4.3.1-min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.localscroll-1.2.7-min.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/stellar.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/jquery.appear.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/validate.js'
		'@SopinetTemplateAmoebaBundle/Resources/public/js/main.js'
		
	%}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	
	{% endjavascripts %}
{% endblock %}
    
</body>
</html>